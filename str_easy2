string itc_even_place(string str){
string new;
int new2 = 0;
 while(str[i] != '\0'){
  if(str[i] % 2 != 0){
 new  = new + str[i];
}else{
new2 = -1;
}
    }
if(new2 = 0){
return new;
}else{
return -1;
    }
 }
 double itc_percent_lower_uppercase(string str){
 int i, otvet;
 int mini;
 int maxi;
 while(str[i] != '\0'){
        if(str[i] < str[i] + 1){
    mini = mini + 1;
        }else{
   maxi = maxi + 1;     
        }
   i++; 
    }
    otvet = maxi  / mini;
 }
